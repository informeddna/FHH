<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration
PUBLIC "-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
    <session-factory name="java:/hibernate/SessionFactory" >
        <property name="hibernate.connection.datasource">java:/fhh-ds</property>
        <property name="hibernate.current_session_context_class">managed</property>
        <property name="hibernate.transaction.factory_class">org.hibernate.transaction.CMTTransactionFactory</property>
        <property name="hibernate.transaction.manager_lookup_class">org.hibernate.transaction.JBossTransactionManagerLookup</property>
        <property name="hibernate.dialect">${hibernate.dialect}</property>
        <property name="hibernate.jdbc.batch_size">25</property>
        <property name="hibernate.jdbc.batch_versioned_data">true</property>
        <property name="hibernate.cache.use_second_level_cache">true</property>
        <property name="hibernate.cache.provider_class">net.sf.ehcache.hibernate.SingletonEhCacheProvider</property>
        <property name="hibernate.cache.use_query_cache">true</property>

        <mapping class="gov.hhs.fhh.data.Race" />
        <mapping class="gov.hhs.fhh.data.AsianRaceType" />
        <mapping class="gov.hhs.fhh.data.NativeHawaiianRaceType" />
        <mapping class="gov.hhs.fhh.data.Ethnicity" />
        <mapping class="gov.hhs.fhh.data.HispanicEthnicityType" />
        <mapping class="gov.hhs.fhh.data.DisplayString" />
        
        <mapping class="gov.hhs.mfhp.model.Code" />
        <mapping class="gov.hhs.mfhp.model.CodeSystem" />
        <mapping class="gov.hhs.mfhp.model.DisplayName" />
        <mapping class="gov.hhs.mfhp.model.Observation" />
      
    </session-factory>
</hibernate-configuration>
