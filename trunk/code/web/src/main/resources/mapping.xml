<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">

<mapping>
	<class name="gov.hhs.fhh.data.Person">
	   <map-to xml="patientPerson"/>
       <field name="name" type="string">
           <bind-xml name="formatted" node="attribute" location="name" />
       </field>
       <field name="dateOfBirth" type="string" handler="gov.hhs.fhh.web.util.DateFieldHandler">
           <bind-xml name="value" node="attribute" location="birthTime" />
       </field>
       <field name="multipleBirthIndicator" type="string">
            <bind-xml name="value" node="attribute" location="multipleBirthIndicator" />
       </field>
       <field name="genderNode" type="gov.hhs.fhh.data.GenderNode">
            <bind-xml name="administrativeGenderCode"/>
       </field>
       <field name="ethnicities" type="gov.hhs.fhh.data.Ethnicity" collection="arraylist">
            <bind-xml name="ethnicGroupCode"/>
       </field>
       <field name="races" type="gov.hhs.fhh.data.Race" collection="arraylist">
            <bind-xml name="raceCode"/>
       </field>
       <field name="clinicalObservationsNode" type="gov.hhs.fhh.data.util.ClinicalObservationsNode">
            <bind-xml name="subjectOf2" />
       </field>
       <field name="relatives" type="gov.hhs.fhh.data.Relative" collection="arraylist">
            <bind-xml name="relative"/>
       </field>
	</class>
    <class name="gov.hhs.fhh.data.AbstractCodeable">
       <field name="code" type="string">
           <bind-xml name="code" node="attribute"/>
       </field>
       <field name="codeSystemName" type="string">
           <bind-xml name="codeSystemName" node="attribute"/>
       </field>
       <field name="displayName" type="string">
           <bind-xml name="displayName" node="attribute"/>
       </field>
       <field name="id" type="long">
           <bind-xml name="id" node="attribute"/>
       </field>
    </class>
	<class name="gov.hhs.fhh.data.GenderNode">
       <field name="code" type="string">
           <bind-xml name="code" node="attribute"/>
       </field>
       <field name="codeSystemName" type="string">
           <bind-xml name="codeSystemName" node="attribute"/>
       </field>
       <field name="displayName" type="string">
           <bind-xml name="displayName" node="attribute"/>
       </field>
       <field name="originalText" type="string">
           <bind-xml name="originalText" node="attribute"/>
       </field>
	</class>
    <class name="gov.hhs.fhh.data.Ethnicity"  extends="gov.hhs.fhh.data.AbstractCodeable">
    </class>
    <class name="gov.hhs.fhh.data.HispanicEthnicityType"  extends="gov.hhs.fhh.data.Ethnicity">
    </class>
    <class name="gov.hhs.fhh.data.Race"  extends="gov.hhs.fhh.data.AbstractCodeable">
    </class>
    <class name="gov.hhs.fhh.data.AsianRaceType"  extends="gov.hhs.fhh.data.Race">
    </class>
    <class name="gov.hhs.fhh.data.NativeHawaiianRaceType"  extends="gov.hhs.fhh.data.Race">
    </class>
    <class name="gov.hhs.fhh.data.util.ClinicalObservationsNode">
       <field name="observations" type="gov.hhs.fhh.data.ClinicalObservation" collection="arraylist">
            <bind-xml name="clinicalObservation" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.ClinicalObservation">
       <field name="code" type="gov.hhs.fhh.data.util.CodeNode">
            <bind-xml name="code"/>
       </field>
       <field name="valueNode" type="gov.hhs.fhh.data.util.ValueNode">
            <bind-xml name="value" />
       </field>
       <field name="dataEstimatedAgeNode" type="gov.hhs.fhh.data.util.DataEstimatedAgeNode">
            <bind-xml name="dataEstimatedAge" location="subject"/>
       </field>
       <field name="cODCodeNode" type="gov.hhs.fhh.data.util.CodeNode">
            <bind-xml name="code" location="sourceOf" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.ValueNode">
       <field name="value" type="string">
            <bind-xml name="value" node="attribute" />
       </field>
       <field name="unit" type="string">
            <bind-xml name="unit" node="attribute" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.DataEstimatedAgeNode">
        <field name="codeNode" type="gov.hhs.fhh.data.util.CodeNode">
            <bind-xml name="code" />
       </field>
       <field name="ageRangeHandler" type="gov.hhs.fhh.data.util.AgeRangeFieldHandler">
            <bind-xml name="value" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.AgeRangeFieldHandler">
       <field name="unit" type="string">
            <bind-xml name="unit" node="attribute"/>
       </field>
       <field name="lowValue" type="string">
            <bind-xml name="value" node="attribute" location="low" />
       </field>
       <field name="highValue" type="string">
            <bind-xml name="value" node="attribute" location="high" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.CodeNode">
   	    <field name="id" type="long">
            <bind-xml name="id" node="attribute"/>
       </field>
       <field name="code" type="string">
            <bind-xml name="code" node="attribute"/>
       </field>
       <field name="codeSystemName" type="string">
            <bind-xml name="codeSystemName" node="attribute"/>
       </field>
       <field name="displayName" type="string">
            <bind-xml name="displayName" node="attribute"/>
       </field>
       <field name="originalText" type="string">
            <bind-xml name="originalText" node="attribute"/>
       </field>
    </class>
    <class name="gov.hhs.fhh.data.Disease"  extends="gov.hhs.fhh.data.AbstractCodeable">
	   <field name="originalText" type="string">
            <bind-xml name="originalText" node="attribute"/>
       </field>
    </class>
    <class name="gov.hhs.fhh.data.CancerDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.DiabetesDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.GastrointestinalDisorderDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.HeartDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.KidneyDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.LungDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>
    <class name="gov.hhs.fhh.data.PsychologicalDisorderDiseaseType" extends="gov.hhs.fhh.data.Disease">
    </class>   
    <class name="gov.hhs.fhh.data.Relative">
	   <field name="relativeCodeNode" type="gov.hhs.fhh.data.util.RelativeCodeNode">
           <bind-xml name="code" />
       </field>
       <field name="relationshipHolderNode" type="gov.hhs.fhh.data.util.RelationshipHolderNode">
           <bind-xml name="relationshipHolder" />
       </field> 
    </class>
    <class name="gov.hhs.fhh.data.util.RelativeCodeNode">
       <field name="code" type="string">
           <bind-xml name="code" node="attribute" />
       </field>
       <field name="codeSystemName" type="string">
           <bind-xml name="codeSystemName" node="attribute" />
       </field>
       <field name="displayName" type="string">
           <bind-xml name="displayName" node="attribute" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.RelationshipHolderNode">
       <field name="parentNode" type="gov.hhs.fhh.data.Relative">
           <bind-xml name="relative" />
       </field>
       <field name="id" type="long">
           <bind-xml name="extention" node="attribute" location="id" />
       </field>
       <field name="name" type="string">
           <bind-xml name="formatted" node="attribute" location="name" />
       </field>
       <field name="birthTime" type="string">
           <bind-xml name="value" node="attribute" location="birthTime" />
       </field>
       <field name="dataEstimatedAgeNode" type="gov.hhs.fhh.data.util.DataEstimatedAgeNode">
            <bind-xml name="dataEstimatedAge"/>
       </field>
       <field name="deceasedIndicator" type="string">
            <bind-xml name="value" node="attribute" location="deceasedIndCode" />
       </field>
       <field name="multipleBirthIndicator" type="string">
            <bind-xml name="value" node="attribute" location="multipleBirthIndicator" />
       </field>
       <field name="genderNode" type="gov.hhs.fhh.data.GenderNode">
            <bind-xml name="administrativeGenderCode"/>
       </field>
       <field name="ethnicities" type="gov.hhs.fhh.data.Ethnicity" collection="arraylist">
            <bind-xml name="ethnicGroupCode"/>
       </field>
       <field name="races" type="gov.hhs.fhh.data.Race" collection="arraylist">
            <bind-xml name="raceCode"/>
       </field>
       <field name="deceasedEstimatedAgeNode" type="gov.hhs.fhh.data.util.DeceasedEstimatedAgeNode">
            <bind-xml name="deceasedEstimatedAge" location="subjectOf1"/>
       </field>
       <field name="clinicalObservationsNode" type="gov.hhs.fhh.data.util.ClinicalObservationsNode">
            <bind-xml name="subjectOf2" />
       </field>
    </class>
    <class name="gov.hhs.fhh.data.util.DeceasedEstimatedAgeNode">
        <field name="codeNode" type="gov.hhs.fhh.data.util.CodeNode">
            <bind-xml name="code" />
       </field>
       <field name="ageRangeHandler" type="gov.hhs.fhh.data.util.AgeRangeFieldHandler">
            <bind-xml name="value" />
       </field>
    </class>
</mapping>